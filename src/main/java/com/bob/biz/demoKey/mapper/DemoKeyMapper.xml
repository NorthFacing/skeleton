<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bob.biz.demoKey.mapper.DemoKeyMapper">

	<resultMap id="BaseResultMap" type="com.bob.biz.demoKey.model.DemoKey">
		<result column="id" property="id" />
		<result column="lock_id" property="lockId" />
		<result column="brand" property="brand" />
		<result column="create_time" property="createTime" />
		<result column="create_user" property="createUser" />
		<result column="update_time" property="updateTime" />
		<result column="update_user" property="updateUser" />
	</resultMap>

	<resultMap id="voResultMap" type="com.bob.biz.demoKey.model.DemoKeyVo"
		extends="BaseResultMap">
	</resultMap>

	<sql id="tabaleName">demoKey as t</sql>

	<sql id="baseColumn">
		t.id,
		t.lock_id,
		t.brand,
		t.create_time,
		t.create_user,
		t.update_time,
		t.update_user
	</sql>

	<sql id="whereCondition">
		<where>

			<include refid="com.bob.core.base.mapper.BaseMapper.baseCondition" />
			
			<if test=" lockId !=null and lockId !='' ">
				and t.lock_id = #{lockId}
			</if>
			<if test=" brand !=null and brand !='' ">
				and t.brand like '%${brand}%'
			</if>

		</where>
	</sql>

	<select id="getList" parameterType="com.bob.biz.demoKey.model.DemoKey"
		resultMap="BaseResultMap">
		select
		<include refid="baseColumn" />
		from
		<include refid="tabaleName" />
		<include refid="whereCondition" />
	</select>

</mapper>