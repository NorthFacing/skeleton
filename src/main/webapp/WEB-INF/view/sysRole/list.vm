<!DOCTYPE html>
<html lang="zh-CN">
<head>
    #parse("/WEB-INF/view/layout/base.vm")
    #parse("/WEB-INF/view/layout/header.vm")
  <title>医院管理</title>
</head>
<body>
<div class="container-fluid">
    #parse("/WEB-INF/view/layout/navbar.vm")
  <div class="row">
    <div class="col-md-2">
        #parse("/WEB-INF/view/layout/menu.vm")
    </div>
    <div class="col-md-10">
      <div class="row">
        <ol class="breadcrumb">
          <li><i class="glyphicon glyphicon-home"></i></li>
          <li><a href="$!{path}/admin/login">首页</a></li>
          <li><a href="#">医院管理</a></li>
        </ol>
      </div>

      <table id="grid-data" class="table table-hover table-striped">
        <thead>
        <tr>
          <th data-column-id="id" data-sortable="false" data-identifier="true" data-visible="false">ID
          </th>
          <th data-column-id="name" data-sortable="false" data-formatter="nameFormatter">名称</th>
          <th data-column-id="description" data-sortable="false" data-formatter="typeFormatter">描述</th>
          <th data-formatter="status" data-sortable="false">状态</th>
          <th data-formatter="commands" data-sortable="false">操作</th>
        </tr>
      </table>
    </div>
  </div>
</div>
</div>

    #parse("/WEB-INF/view/layout/bottom.vm")

<script src="$!{path}/resources/sysRole/list.js?staticVersion=$!{staticVersion}" type="text/javascript"></script>

</body>
</html>
